<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="default_padding_Top8Left12">
        <item name="android:paddingTop">@dimen/item_parent_paddingTopBottom</item>
        <item name="android:paddingBottom">@dimen/item_parent_paddingTopBottom</item>
        <item name="android:paddingRight">@dimen/item_parent_paddingLeftRight</item>
        <item name="android:paddingLeft">@dimen/item_parent_paddingLeftRight</item>
    </style>

    <style name="default_padding_Top12Left8">
        <item name="android:paddingTop">@dimen/item_parent_paddingLeftRight</item>
        <item name="android:paddingBottom">@dimen/item_parent_paddingLeftRight</item>
        <item name="android:paddingRight">@dimen/item_parent_paddingTopBottom</item>
        <item name="android:paddingLeft">@dimen/item_parent_paddingTopBottom</item>
    </style>

    <style name="default_padding_Top16Left8">
        <item name="android:paddingTop">@dimen/default_16</item>
        <item name="android:paddingBottom">@dimen/default_16</item>
        <item name="android:paddingRight">@dimen/item_parent_paddingTopBottom</item>
        <item name="android:paddingLeft">@dimen/item_parent_paddingTopBottom</item>
    </style>

    <style name="text_default">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_default</item>
        <item name="android:text">@string/hello_world</item>
    </style>

    <style name="edt_default">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_default</item>
        <item name="android:textColorHint">@color/text_hint</item>
    </style>

    <style name="button_default">
        <item name="android:minHeight">18dp</item>
        <item name="android:minWidth">18dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/text_default</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:text">@string/hello_world</item>
    </style>

    <style name="imagebutton_default">
        <item name="android:minHeight">0dp</item>
        <item name="android:background">@color/transparent</item>
    </style>

    <style name="button_default_noText">
        <item name="android:minHeight">0dp</item>
    </style>

    <style name="text_default_s_gray">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:text">@string/hello_world</item>
    </style>

    <style name="text_default_gray">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:text">@string/hello_world</item>
    </style>

    <style name="tab_menu_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">10sp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:textColor">@color/selecoter_color_main_tab</item>
    </style>

    <style name="MyTablayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="MaterialDialogSheet" parent="@android:style/Theme.Dialog">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowAnimationStyle">@style/PopupAnimationBottomCart</item>
    </style>

    <style name="CashOrder_Codeno_NUM">
        <item name="android:textColor">@color/text_default</item>
        <item name="android:background">@drawable/selector_codeno_num_bg</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:layout_margin">0.5dp</item>
        <item name="android:textSize">20sp</item>
    </style>

    <style name="edt_foodedit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_default</item>
        <item name="android:textColorHint">@color/text_hint</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    <style name="button_submit">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/hello_world</item>
        <item name="android:background">@drawable/selector_bg_round_orange_s</item>
    </style>



    <style name="loading_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/d_bg_round_bgdefaults_s</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="text_main_my_item">
        <item name="android:paddingLeft">@dimen/default_16</item>
        <item name="android:paddingRight">@dimen/default_16</item>
        <item name="android:paddingTop">@dimen/default_12</item>
        <item name="android:paddingBottom">@dimen/default_12</item>
        <item name="android:textColor">@color/text_default</item>
        <item name="android:background">@drawable/d_line_top_grays_whitebg</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">@dimen/default_16</item>
    </style>

    <declare-styleable name="MaxHeightView">
        <attr name="mhv_HeightRatio" format="reference|float" />
        <attr name="mhv_HeightDimen" format="reference|dimension" />
    </declare-styleable>

    <style name="button_orange" parent="Theme.AppCompat.Light">
        <item name="colorControlHighlight">@drawable/d_bg_round_orange_s</item>
        <item name="colorButtonNormal">@drawable/d_bg_round_oranges_s</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_hint" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/text_gray</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text_hint_out" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/text_red</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="PopupAnimationTop">
        <item name="android:windowEnterAnimation">@anim/popup_top_in</item>
        <item name="android:windowExitAnimation">@anim/popup_top_out</item>
    </style>

    <style name="PopupAnimationBottom">
        <item name="android:windowEnterAnimation">@anim/popup_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/popup_bottom_out</item>
    </style>

    <style name="PopupAnimationBottomCart">
        <item name="android:windowEnterAnimation">@anim/popup_cart_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/popup_cart_bottom_out</item>
    </style>
    
     <style name="NotificationTitle">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="NotificationContent">
        <item name="android:textColor">?android:attr/textColorPrimary</item>
    </style>

    <style name="BaseView">
    </style>

</resources>